<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>InjectionAnalyzer</id>
    <version>0.9.0.0</version>
    <title>InjectionAnalyzer</title>
    <authors>Bejenari Marian</authors>
    <owners>Bejenari Marian</owners>
    <licenseUrl>https://github.com/Kerego/InjectionAnalyzer/blob/master/License.md</licenseUrl>
    <projectUrl>https://github.com/Kerego/InjectionAnalyzer</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>rovides diagnostics and code fixes for uninitialized private fields.</description>
    <releaseNotes>Summary of changes made in this release of the package.</releaseNotes>
    <copyright>Copyright</copyright>
    <tags>InjectionAnalyzer, analyzers, roslyn, injection</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>